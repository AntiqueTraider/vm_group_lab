    "def cholesky(A):\n",
    "    L = np.zeros((len(A), len(A)))\n",
    "    for i in range(len(A)):\n",
    "        for j in range(i+1):\n",
    "            s = sum(L[i][k] * L[j][k] for k in range(j))\n",
    "            L[i][j] = sqrt(A[i, i] - s) if (i == j) else  (1.0 / L[j][j] * (A[i, j] - s))\n",
    "    return L"
